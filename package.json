{
  "name": "qcloud-iot-panel-component",
  "version": "0.0.40",
  "description": "腾讯连连开发面板组件",
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "docs": "npx dumi dev",
    "docs:build": "npx dumi build",
    "lint": "eslint src",
    "build": "rm -rf ./lib && npm run build:h5 -- --environment=production && npm run build:weapp -- --environment=production",
    "dev": "rm -rf ./lib && concurrently \"npm run build:h5 -- --environment=development --watch \" \"npm run build:weapp -- --environment=development --watch\"",
    "build:weapp": "npx rollup -c --platform=weapp",
    "build:h5": "npx rollup -c --platform=h5"
  },
  "keywords": [
    "iot",
    "device panel"
  ],
  "files": [
    "lib/**/*.js",
    "lib/**/*.css",
    "lib/**/*.d.ts",
    "es/*"
  ],
  "homepage": "https://github.com/tencentyun/iot-panel-component",
  "bugs": {
    "url": "https://github.com/tencentyun/iot-panel-component/issues"
  },
  "author": "iot-frontend",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-replace": "^3.0.0",
    "@types/node": "^16.9.1",
    "@types/react": "16.9.55",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "dumi": "^1.1.28",
    "dumi-theme-mobile": "^1.1.11",
    "esbuild": "^0.12.28",
    "esbuild-plugin-alias-path": "^0.2.3",
    "eslint-plugin-react": "^7.25.1",
    "less": "^4.1.1",
    "postcss": "^8.3.6",
    "rollup": "^2.56.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-esbuild": "^4.5.0",
    "rollup-plugin-import-css": "^3.0.0",
    "rollup-plugin-inject-process-env": "^1.3.1",
    "rollup-plugin-multi-input": "^1.3.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-static-import": "^0.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "is-url": "^1.2.4",
    "lodash.chunk": "^4.2.0",
    "omit.js": "^2.0.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "@tarojs/components": "^3.3.7",
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  }
}
